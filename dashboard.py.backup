
@app.callback(
    Output("contact-founder-modal", "is_open", allow_duplicate=True),
    [Input("close-contact-modal", "n_clicks")],
    [State("contact-founder-modal", "is_open")],
    prevent_initial_call=True
)
def close_modal(n, is_open):
    if n and is_open:
        return False
    return is_open
